{
  "name": "lazycommitai",
  "displayName": "LazyCommitAI",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lazycommitai.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "extension.commitAndPush",
        "title": "Commit and Push Changes"
      },
      {
        "command": "extension.openDashboard",
        "title": "Open Lazy Commit Dashboard"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.16.12",
    "@types/vscode": "^1.94.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@huggingface/transformers": "^3.0.0",
    "dotenv": "^16.4.5",
    "groq-sdk": "^0.7.0",
    "react-hot-toast": "^2.4.1",
    "webpack-cli": "^5.1.4"
  }
}
